{
  "name": "obsidian-calendar-plugin",
  "version": "1.0.0",
  "main": "src/main.tsx",
  "license": "MIT",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "dev:w": "concurrently \"node esbuild.config.mjs\" \"yarn copyAndWatchMainJS\" \"yarn copyAndWatchStylesJS\" \"yarn copyAndWatchManifestJS\"",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "buildAndCopy": "yarn build && yarn copyMainJS && yarn copyStylesJS && yarn copyManifestJS",
    "build:dev": "concurrently \"yarn dev\" \"yarn copyMainJS\" \"yarn copyStylesJS\" \"yarn copyManifestJS\"",
    "stage": "yarn format && yarn lint && yarn test",
    "lint": "eslint src/",
    "format": "prettier src/ --write",
    "test": "jest --config jest.config.mjs",
    "copyAndWatchMainJS": "yarn copyMainJS -w",
    "copyAndWatchStylesJS": "yarn copyStylesJS -w",
    "copyAndWatchManifestJS": "yarn copyManifestJS -w",
    "copyMainJS": "cpx \"dist\\main.js\" \"test-vault\\.obsidian\\plugins\\obsidian-calendar-plugin\"",
    "copyStylesJS": "cpx \"dist\\styles.css\" \"test-vault\\.obsidian\\plugins\\obsidian-calendar-plugin\"",
    "copyManifestJS": "cpx \"manifest.json\" \"test-vault\\.obsidian\\plugins\\obsidian-calendar-plugin\"",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "builtin-modules": "^4.0.0",
    "concurrently": "^9.0.1",
    "cpx": "^1.5.0",
    "esbuild": "^0.24.0",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-obsidian": "^0.0.1",
    "obsidian": "^1.7.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "i18next": "^23.16.2",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0"
  }
}
